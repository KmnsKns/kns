pipeline {
    agent any
    tools {
        maven "maven3.9.2"
    }
    stages {
        stage('GIT CHECKOUT') {
            steps {
                git url:'https://github.com/KmnsKns/kns.git'
            }
            }
        stage('BUILD') {
            steps {
                sh 'mvn clean package'
            }
        }
         stage('REPORT') {
             steps {
                 sh 'mvn sonar:sonar'
        }
    }
}
}
